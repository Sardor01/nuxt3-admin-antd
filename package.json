    "pinia": "^2.0.33",
    "vee-validate": "^4.8.4",
    "yup": "1.0.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.4",
    "@commitlint/cli": "^17.5.1",
    "@commitlint/config-conventional": "^17.4.2",
    "@nuxtjs/i18n": "8.0.0-beta.9",
    "@tailwindcss/forms": "^0.5.3",
    "@tailwindcss/typography": "^0.5.9",
    "@types/node": "^18.15.11",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.37.0",
    "husky": "^8.0.0",
    "less": "^4.1.3",
    "lint-staged": "^13.2.0",
    "nuxt": "^3.3.3",
    "postcss": "^8.4.21",
